    "build:main": "tsc -p src/main/tsconfig.json",
    "build:preload": "tsc -p src/preload/tsconfig.json",
    "build:renderer": "tsc -p src/renderer/tsconfig.json",
    "copy-assets": "mkdir -p dist/renderer && cp src/renderer/*.html src/renderer/*.css dist/renderer/",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer && npm run copy-assets",
    "start": "unset ELECTRON_RUN_AS_NODE && npm run build && electron .",
